{
  "name": "yawb",
  "version": "0.1.0",
  "description": "Yet Another Web Boilerplate",
  "private": true,
  "scripts": {
    "build":
      "cross-env REACT_APP_VERSION=$npm_package_version react-scripts build",
    "clean": "rm -rf build coverage",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "lint": "yarn lint-js && yarn lint-format",
    "lint-format": "prettier --list-different \"**/*.{js,json,md}\"",
    "lint-js": "eslint .",
    "start":
      "cross-env REACT_APP_VERSION=$npm_package_version react-scripts start",
    "test":
      "cross-env REACT_APP_VERSION=$npm_package_version react-scripts test --env=jsdom"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "i18next": "^10.3.0",
    "i18next-browser-languagedetector": "^2.1.0",
    "i18next-xhr-backend": "^1.5.1",
    "jss": "^9.8.0",
    "jss-preset-default": "^4.3.0",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-i18next": "^7.3.6",
    "react-jss": "^8.3.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "redux": "^3.7.2",
    "typeface-roboto": "^0.0.54"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "prettier": "^1.10.2",
    "react-test-renderer": "^16.2.0"
  }
}
